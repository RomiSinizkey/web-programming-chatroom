<%
const scriptsList =
        (typeof scripts !== 'undefined' && Array.isArray(scripts))
                ? scripts
                : [];
%>

<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"></script>

<% scriptsList.forEach(src => { %>
    <script src="<%= src %>"></script>
<% }) %>

</body>
</html>
